{
  "$schema": "https://turborepo.dev/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ],
      "env": [
        "DATABASE_URL",
        "NODE_ENV",
        "NEXT_PUBLIC_*",
        "BETTER_AUTH_BASE_URL",
        "BETTER_AUTH_SECRET",
        "ENCRYPTION_KEY",
        "GITHUB_CLIENT_ID",
        "GITHUB_CLIENT_SECRET",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "GEMINI_API_KEY",
        "OPEN_ROUTER_API_KEY"
      ]
    },
    "dev:all": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "DATABASE_URL",
        "NODE_ENV",
        "NEXT_PUBLIC_*",
        "BETTER_AUTH_BASE_URL",
        "BETTER_AUTH_SECRET",
        "ENCRYPTION_KEY",
        "GITHUB_CLIENT_ID",
        "GITHUB_CLIENT_SECRET",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET"
      ]
    },
    "start": {
      "dependsOn": [
        "build"
      ],
      "cache": false,
      "persistent": true,
      "env": [
        "DATABASE_URL",
        "NODE_ENV",
        "NEXT_PUBLIC_*",
        "BETTER_AUTH_BASE_URL",
        "BETTER_AUTH_SECRET",
        "ENCRYPTION_KEY",
        "GITHUB_CLIENT_ID",
        "GITHUB_CLIENT_SECRET",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "generate": {
      "dependsOn": [
        "^generate"
      ],
      "cache": false,
      "env": [
        "DATABASE_URL"
      ]
    },
    "db:migrate:dev": {
      "cache": false,
      "persistent": true,
      "interactive": true,
      "env": [
        "DATABASE_URL"
      ]
    },
    "db:migrate:reset": {
      "cache": false,
      "env": [
        "DATABASE_URL"
      ]
    },
    "db:migrate:deploy": {
      "cache": false,
      "env": [
        "DATABASE_URL"
      ]
    },
    "db:push": {
      "cache": false,
      "env": [
        "DATABASE_URL"
      ]
    },
    "db:studio": {
      "cache": false,
      "env": [
        "DATABASE_URL"
      ]
    },
    "db:seed": {
      "cache": false,
      "env": [
        "DATABASE_URL"
      ]
    },
    "worker:dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "DATABASE_URL",
        "GEMINI_API_KEY",
        "OPEN_ROUTER_API_KEY",
        "ENCRYPTION_KEY"
      ]
    },
    "producer:dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "DATABASE_URL"
      ]
    }
  }
}